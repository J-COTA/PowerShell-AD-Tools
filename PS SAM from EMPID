# Step 1: Read all IDs and store them in a variable
$IDs = Get-Content "C:\Users\usrname\Documents\list.txt"

# Step 2: Get all users and their employeeID
Get-ADUser -Filter "*" -Property EmployeeID |

# Step 3: Pass the output into a filter that only passes on those users, whose ID is listed
   Where-Object { $IDs -contains $_.employeeID } |

# Step 4: Get all information on the remaining users
   Get-ADUser -Property "*" |

# Step 5: Export to csv
   Export-Csv "C:\export\employeedata.csv"
